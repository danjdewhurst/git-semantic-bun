{
  "name": "git-semantic-bun",
  "version": "0.1.0",
  "description": "Local semantic git commit search CLI built with Bun + TypeScript",
  "type": "module",
  "bin": {
    "gsb": "./src/cli.ts"
  },
  "scripts": {
    "build": "bun build ./src/cli.ts --outdir dist --target bun",
    "test": "bun test",
    "typecheck": "bunx tsc --noEmit",
    "lint": "bunx @biomejs/biome check ."
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "^1.2.5",
    "typescript": "^5.7.3"
  },
  "engines": {
    "bun": ">=1.3.9"
  },
  "license": "MIT"
}
